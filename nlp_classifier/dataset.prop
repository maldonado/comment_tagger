#
# Features
#
useClassFeature=true
1.useNGrams=false
1.useSplitWords
1.splitWordsRegexp "\s"
1.binnedLengths=10,20,30
#
# Printing
#
printClassifier=HighWeight
printClassifierParam=60000
displayAllAnswers=true
#
# Mapping
#
goldAnswerColumn=0
displayedColumn=1
#
# Optimization
#
intern=true
sigma=3
useQN=true
QNsize=15
tolerance=1e-4
#
# Training input
#
trainFile=./classified_seq.train
testFile= ./classified_seq.test
